    function delModule(condition) {
        var del;
        var mod_id;
        if (condition.typeName == 'pic' || condition.typeName == 'slidepic' || condition.typeName == 'slide' || condition.typeName =='carousel_pic') {
            del = condition.type+condition.module_id+condition.id;
            mod_id = condition.type+condition.module_id;
        } else {
            del = condition.type+condition.module_id;
        }
        if(condition.is_start == 1 && condition.is_stop == 1  && condition.start && condition.stop){
            if (_now < condition.start || _now > condition.stop) {
              // $("#"+del).css('display','none');
              var mod_y_len = $('#' + mod_id).find(".defaultBlock").length;
              $("#"+del).remove();
              if (condition.typeName =='pic') {
                var mod_rem_len = $('#' + mod_id).find(".defaultBlock").length;
                mod_rem_col(mod_id,mod_y_len,mod_rem_len);
              }
            }
        }
        if (condition.is_start == 1 && condition.is_stop == 0 && condition.start) {
            if (_now < condition.start) {
              var mod_y_len = $('#' + mod_id).find(".defaultBlock").length;
              $("#"+del).remove();
              if (condition.typeName =='pic') {
                var mod_rem_len = $('#' + mod_id).find(".defaultBlock").length;
                mod_rem_col(mod_id,mod_y_len,mod_rem_len);
              }
            }
        }
        if (condition.is_start == 0 && condition.is_stop == 1 && condition.stop) {
            if (_now > condition.stop) {
              var mod_y_len = $('#' + mod_id).find(".defaultBlock").length;
              $("#"+del).remove();
              if (condition.typeName =='pic') {
                var mod_rem_len = $('#' + mod_id).find(".defaultBlock").length;
                mod_rem_col(mod_id,mod_y_len,mod_rem_len);
              }
            }
        }

    }
    function mod_rem_col(mod_id,mod_y_len,mod_rem_len){
      var rem_class="";
      if(mod_y_len == 2){
        rem_class = "mod-col-2-4";
      }else{
        rem_class = "mod-col-" + mod_y_len + "-" + mod_y_len;
      }
      // console.log("模块id:" + mod_id + "；class:" + rem_class);
      if(mod_rem_len == 2){
          $("#"+mod_id).children().addClass("mod-col-" + mod_rem_len + "-4").removeClass(rem_class);
      }else{
          $("#"+mod_id).children().addClass("mod-col-" + mod_rem_len + "-" + mod_rem_len).removeClass(rem_class);
      }
    }
