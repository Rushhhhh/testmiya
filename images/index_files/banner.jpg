    if (syncTimer.length > 0) {
      $.each(syncTimer,function(name,value) {
          if ($.type(value) == 'array') 
          {
            $.each(value,function(kpic,pic) {
              delModule(pic);
            });
          } 
          else 
          {
             delModule(value)
