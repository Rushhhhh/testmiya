$.bt_validate.fn={required:function(e){return null!=e&&""!=e},email:function(e){return/^[a-z0-9-_\.]+@[a-z0-9-_\.]+\.[a-z]{2,4}$/.test(e)},www:function(e){return/^(http:\/\/)|(https:\/\/)[a-z0-9\/\.-_]+\.[a-z]{2,4}$/.test(e)},date:function(e){return/^[\d]{2}\/[\d]{2}\/[\d]{4}$/.test(e)},time:function(e){return/^[\d]{2}:[\d]{2}(:{0,1}[\d]{0,2})$/.test(e)},datetime:function(e){return/^[\d]{2}\/[\d]{2}\/[\d]{4} [\d]{2}:[\d]{2}:{0,1}[\d]{0,2}$/.test(e)},number:function(e){return/^[\d]+$/.test(e)},"float":function(e){return/^([\d]+)|(\d]+\.[\d]+)$/.test(e)},equal:function(e,t){return e==t},min:function(e,t){return Number(e)>=t},max:function(e,t){return Number(e)<=t},between:function(e,t,n){return Number(e)>=t&&Number(e)<=n},length_min:function(e,t){return e.length>=t},length_max:function(e,t){return e.length<=t},length_between:function(e,t,n){return e.length>=t&&e.length<=n},cell:function(e){return/^(1(([35][0-9])|(47)|[8][0126789]))\d{8}$/.test(e)},username:function(e){return/^[a-zA-Z0-9_]{6,16}$/.test(e)},mobile:function(e){return/^1([3578][0-9]|4[57])\d{8}$/.test(e)}},$.bt_validate.text={required:"不能为空",email:"需是一个合法的email",www:"The value is not valid http string",date:"The value is not valid date",time:"The value is not valid time",datetime:"The value is not valid datetime",number:"The value is not valid number","float":"The value is not valid floating point number",equal:'The value must be equal to "%1"',min:"The value must be equal or greater than %1",max:"The value must be equal or less than %1",between:"The value must be between %1 and %2",length_min:"长度需大于等于 %1",length_max:"The length of the value must be equal or less than %1",length_between:"输入的长度范围需介于 %1 和 %2 之间",cell:"请输入正确的手机号码",username:"用户名为6到16位字母或数字且不能全为数字",mobile:"需使用手机号登录，如无手机号可通过蜜芽APP绑定"};